<!DOCTYPE html>
<html lang="en">
<title><%=datalist[0]['resourceName']%></title>
<%include head.ejs%>
<style type="text/css">
.center-icon {
    left: 45%;
    position: absolute;
}
</style>
<body class="secondbody">
  <%include modal.ejs%>
  <div class="container project-container">
    <div class="row secondpage">
        <section class="col-xs-12 mininav navbar">
            <!--<a class="navbar-brand pull-right" onclick="save()" >-->
            <a class="navbar-brand pull-right" onclick="" >
              <img src="http://file.interestm.com/static/img/save2.png" alt="logo" class="m_logo" >
            </a>
            <a class="navbar-brand" href="/" >
              <img src="http://file.interestm.com/static/img/m_logo.png" alt="logo" class="m_logo" >
            </a>
        </section>
        <section class="project-waterfall waterfall col-xs-12" id="project-waterfall" > 
          <%datalist.forEach(function(item){%>  
            <div class="col-xs-12 col-sm-6" style="padding: 4px; margin-bottom: 1em">
              <a href=<%=item.link%>  target="_blank"> <img src=<%=item.imageUrl%> style="width: 100%;border-radius: 10px;" alt=<%=item.imageName%>  > </a>
              <!-- IMG LOGO <img src=<%=item.typeImg%>    class="logotype topleft">-->
              <p style="text-align:center;margin-top:5px;"><%=item.resourceDesc%> </p>
            </div>
         <%})%>  
        </section>  
        <section class="col-xs-12 col-xs-12 minifoot">
          <!--social module-->
          <a class="navbar-brand pull-right"  onclick="show('.social-share')">
            <img src="http://file.interestm.com/static/img/share.png" alt="logo" class="m_logo" >
          </a>
          <div class="social-position hidden" data-mobile-sites="weibo,qq,qzone,tencent">
            <!--<a href="javascript:;" class="social-share-icon icon-heart"></a>-->
          </div>
          <!-- link module-->
          <a class="navbar-brand center-icon"  onclick="show('#prolink')" >
            <img src="http://file.interestm.com/static/img/hotlink.png" alt="logo" class="m_logo" >
          </a>
          <ul class="share-link hidden center-block" id="prolink"> 
            <ol><a href="http://www.suryresort.com">酒店预订</a></ol>
            <ol><a href="/profile">施工合作</a></ol>
            <ol><a href="http://www.atelier-xuk.com.cn/">设计合作</a></ol>
          </ul>  
          <a class="navbar-brand pull-left" href="/profile" >
            <img src="http://file.interestm.com/static/img/userTm.jpg" alt="logo" class="m_logo" >
          </a>
        </section>
    </div>
  </div>
</body>
    <%include foot.ejs%>
    <script> 
    $(document).ready(function () {
        initSocial(".social-position");
      })
    </script>
</html>
